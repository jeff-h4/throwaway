    "jest": "^22.4.3",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "webpack-dev-server": "2.11.2"
  },
  "scripts": {
    "lint": "tslint -c config/tslint.json 'app/javascript/**/*.{ts,tsx}'",
    "test": "jest"
  },
  "jest": {
    "roots": [
      "app/javascript"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/javascript"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "app/javascript/.*\\.spec.(ts|tsx|js)$"
  }
